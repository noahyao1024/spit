{
    "admin":{
        "disabled":true,
        "listen":"0.0.0.0:2019",
        "enforce_origin":false,
        "origins":[
            ""
        ]
    },
    "apps":{
        "http":{
            "servers":{
                "static":{
                    "idle_timeout":30000000000,
                    "listen":[
                        "0.0.0.0:PORT"
                    ],
                    "max_header_bytes":10240000,
                    "read_header_timeout":10000000000,
                    "routes":[
                        {
                            "match":[
                                {
                                    "path_regexp":{
                                        "name":"proxy",
                                        "pattern":"^/UPSTREAM_PATTERN"
                                    }
                                }
                            ],
                            "handle":[
                                {
                                    "handler":"reverse_proxy",
                                    "upstreams":[
                                        {
                                            "dial":"UPSTREAM_HOST"
                                        }
                                    ],
                                    "headers":{
                                        "request":{
                                            "add":{
                                                "Host":[
                                                    "UPSTREAM_HOST"
                                                ]
                                            }
                                        }
                                    }
                                }
                            ]
                        },
                        {
                            "match":[

                            ],
                            "handle":[
                                {
                                    "handler":"file_server",
                                    "root":"STATIC",
                                    "browse":{
                                        "template_file":""
                                    }
                                }
                            ],
                            "terminal":true
                        }
                    ]
                }
            }
        }
    }
}